
Host sentry
  HostName vpn.metamaya.com
  User dunc
  IdentityFile ~/.ssh/id_ed25519
  ForwardAgent yes

Host servo
  HostName servo
  User dunc
  IdentityFile ~/.ssh/id_ed25519
  ProxyJump sentry

Host neon
  HostName neon
  User dunc
  IdentityFile ~/.ssh/id_ed25519
  ProxyJump sentry

HashKnownHosts no
StrictHostKeyChecking accept-new

Host *
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_ed25519
